{
  "name": "gocommander",
  "version": "1.0.0",
  "description": "A Go-based port of Commander.js compiled to WebAssembly for high-performance CLI applications",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib/",
    "wasm/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "node scripts/build-wasm.js",
    "build:js": "rollup -c rollup.config.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib",
    "test": "jest",
    "test:wasm": "node scripts/test-wasm.js",
    "dev": "npm run build && npm run test",
    "clean": "rimraf lib wasm dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "cli",
    "command-line",
    "commander",
    "go",
    "wasm",
    "webassembly",
    "performance",
    "argument-parser"
  ],
  "author": "GoCommander Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gocommander/gocommander.git"
  },
  "bugs": {
    "url": "https://github.com/gocommander/gocommander/issues"
  },
  "homepage": "https://github.com/gocommander/gocommander#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "node": ">=14.0.0"
  }
}